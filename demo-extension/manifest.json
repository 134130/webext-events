{
	"name": "webext-events",
	"version": "0.0.0",
	"manifest_version": 2,
	"background": {
		"scripts": ["background.js"]
	},
	"content_scripts": [
		{
			"all_frames": true,
			"matches": ["https://accepted-ephiframe.vercel.app/*"],
			"js": ["contentscript.js"]
		}
	],
	"sandbox": {
		"pages": ["sandbox.html"]
	},
	"web_accessible_resources": ["sandbox.html"]
}
